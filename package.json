{
  "name": "visual_graph",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .ts && prettier --check './src/**/*.ts'",
    "lint:fix": "eslint src --ext .ts --fix && prettier --write './src/**/*.ts'",
    "prepare": "husky install",
